<view class="page">
  <view wx:if="{{hasResult}}">
    <view class="weui-cells">
      <navigator bindtap="viewExamScoreDetail" data-examScore="{{examScore}}" wx:for="{{examScores}}"
                 wx:for-item="examScore" wx:key="examScore" class="weui-cell weui-cell_access"
                 hover-class="weui-cell_active">
        <view class="weui-cell__bd">{{examScore.courseName}}</view>
        <view class="weui-cell__ft weui-cell__ft_in-access" wx:if="{{examScore.passed}}">{{examScore.score}}</view>
        <view class="weui-cell__ft weui-cell__ft_in-access red" wx:else >{{examScore.score}}</view>
      </navigator>
    </view>
  </view>
  <view wx:else class="container">
    <view class="icon-box">
      <icon class="icon-box-img" type="warn" size="93" color="#C9C9C9"></icon>
    </view>
    <view>
      <text class="icon-box-title">未查询到结果</text>
    </view>
  </view>
</view>