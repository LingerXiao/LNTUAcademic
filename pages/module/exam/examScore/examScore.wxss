.icon-box {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.icon-box-img {
    margin: 20rpx;
}

.icon-box-title {
    color: #aaa;
}

.badge-space {
    margin-left: 5px;
    margin-right: 5px;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 100rpx 0;
    box-sizing: border-box;
}

.footer {
    padding: 50rpx 0 15rpx;
}
