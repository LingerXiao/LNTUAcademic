.icon-box {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.icon-box-img {
    margin: 20rpx;
}

.icon-box-title {
    color: #aaa;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 100rpx 0;
    box-sizing: border-box;
}